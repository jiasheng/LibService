<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <session-config>
        <session-timeout>
            3600
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>LibServlet</servlet-name>
        <servlet-class>clientservlet.LibServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LibServlet</servlet-name>
        <url-pattern>/LibServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetDetails</servlet-name>
        <servlet-class>clientservlet.GetDetails</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetDetails</servlet-name>
        <url-pattern>/GetDetails</url-pattern>
    </servlet-mapping>
</web-app>
